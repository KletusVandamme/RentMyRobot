<div class="robot-index-element-image-wrapper">
  <button type="button" class=" btn-video">
  <!-- <button type="button" class="btn btn-default btn-sm btn-video"> -->
    Video  <span class="glyphicon glyphicon-hd-video"></span>
  </button>
  <a href="#/robots/<%= robot.id %>">
    <img src="assets/<%= robot.escape('image_url') %>"
        alt="Picture your robot here1!"
        class="robot-index-image"/>
  </a>
</div>

<left>
